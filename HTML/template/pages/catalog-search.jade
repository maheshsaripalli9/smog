//- Home page
extends _base

block page
  -
    page = {
      language: 'en',
      title: 'Carros | Catalog search',
      appTitle: 'Carros Catalog search',
      description: '',
      id: 'pageCatalog',
      class: 'page-catalog-search page-inner'
    }

block icons

block header
  include ../partials/_header-retailer

